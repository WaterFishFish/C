#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
#include<math.h>
int main() {
    int i;
    int arr[5];
    for (i = 1; i <= 5; i++)
    {
        printf("Enter No.%2d:", i);
        scanf("%d", &arr[i-1]);
    }
    for (int k = 0; k < 5; k++) {
  for (int j = 0; j < 5 -k-1; j++) {
            if (arr[j] > arr[j + 1]) {
                int temp;
                temp = arr[j];
                arr[j] = arr[j + 1];
                arr[j + 1] = temp;

            }
        }
     }
    for (int a = 0; a < 5; a++) {
        printf("%d", arr[a]);
    }
}
