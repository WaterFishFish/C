#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>
int main()
{
	int i;
	int sum = 0;
	int a, b;
	int arr[100];
	for (int i = 0; i < 10; i++) 
		scanf("%d", &arr[i]);
	int max  = arr[0];
	int  min = arr[0];
	for (int i = 1; i < 10; i++) {
		
		if (max < arr[i])
			max = arr[i];
		else if (min > arr[i])
			min = arr[i];
	}
sum = max+min;
	printf("sum=%3d", sum);
}

