#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>
unsigned int Fact(unsigned int n) {
	int i = 1;
	long sum = 1;
	for (i = 1; i <= n; i++) {
		sum = sum * i;
	}
		return sum;
	



}
main() {
	unsigned  int n,temp;
	unsigned long SUM=0;
	printf("Input n(n>0):");
	scanf("%u", &n);
	for (temp = 1; temp <= n; temp++) {
		SUM=SUM+Fact(temp);
	}
	printf("sum = %lu\n", SUM);
}
