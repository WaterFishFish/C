#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
	int main()
	{
		int arr[14] = { 0 };
		int i, n, j = 1;
		
		for (i = 1; i <= 13; i++)
		{
			n = 1;
			do {
				if (j > 13) j = 1;
				if (arr[j]) j++;
				else {
					if (n == i) arr[j] = i;
 					j++;
					n++;
				}
			} while (n <= i);
		}
		for (i = 1; i <= 13; i++)
			printf("%d ", arr[i]);
		printf("\n");
	}
